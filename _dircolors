TERM xterm-color
TERM xterm-debian
TERM alacritty

NORMAL 0                                   # no color code at all
FILE 0                                     # regular file: use no color at all
RESET 0                                    # reset to "normal" color
DIR 38;5;110                               # directory
LINK 38;5;175                              # symbolic link.
MULTIHARDLINK 1                            # regular file with more than one link
FIFO 40;33                                 # pipe
SOCK 1;38;5;138                            # socket
DOOR 1;38;5;139                            # door
BLK 1;38;5;138;48;5;238                    # block device driver
CHR 1;38;5;138;48;5;238                    # character device driver
ORPHAN 38;5;161                            # symlink to nonexistent file, or non-stat'able file
SETUID 48;5;175;38;5;16                    # file that is setuid (u+s)
SETGID 48;5;138;38;5;16                    # file that is setgid (g+s)
CAPABILITY 1;48;5;175;38;5;16              # file with capability
STICKY_OTHER_WRITABLE 48;5;151;38;5;16     # dir that is sticky and other-writable (+t,o+w)
OTHER_WRITABLE 48;5;151;38;5;16            # dir that is other-writable (o+w) and not sticky
STICKY 48;5;110;38;5;16                    # dir with the sticky bit set (+t) and not other-writable
EXEC  1;38;5;151                           # files with execute permission

